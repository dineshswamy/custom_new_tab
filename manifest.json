{
  "manifest_version": 2,

  "name": "Focus",
  "description": "Focus quote within new tab",
  "version": "1.0",
 "background" : {
 	"scripts" :["script.js"]
 	},
  "permissions": [ "geolocation", "history", "idle", "management", "storage", "tabs", "topSites", "chrome://favicon/", "http://*/", "https://*/" ],
   "chrome_url_overrides" : {
   "newtab":"focus.html"
   }

}